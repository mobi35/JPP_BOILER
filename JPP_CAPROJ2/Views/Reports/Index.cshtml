
@{
    ViewData["Title"] = "Privacy Policy";
    Layout = "BackEnd";
}

    <section id="main-content">
        <div class="wrapper">

            <h3><i class="fa fa-angle-right"></i>Reports </h3>
            <!-- BASIC FORM ELELEMNTS -->
            <div class="row mt">
                <div class="col-lg-12">

                    <div class="form-panel">
                        <h1></h1>
                        <div class="row">
                            <div class="col-md-2">
                                Choose Reports
                                <select id="name" name="name" class="form-control" style="width:200px;">
                                    <option value="User Masterlist">User Masterlist</option>
                                    <option value="Sales Report">Sales Reports</option>
                                    <option value="Products Report">Products Report</option>
                                    <option value="Top Customer">Top Customer</option>
                                </select>
                            </div>
                            <div class="col-md-2">
                                Start Date
                                <input id="startDate" name="start" class="form-control" style="width:200px;" type="date" />
                            </div>
                            <div class="col-md-2">
                                End Date
                                <input id="endDate" name="end" class="form-control" style="width:200px;" type="date" />
                            </div>
                            <div class="col-md-2">
                                <br />
                                <button id="showData">ShowData</button>
                            </div>
                        </div>
                        <div id="Reports"></div>

                    </div>

                   
                </div>
                
                
             
                </div></div></section>


                        <script>

                            $('#showData').click(function () {
                                var name = $('#name').val();
                                $.ajax({

                                    url: 'Reports/GetThis/',
                                    type: 'post',
                                    data: {name:name},
                                    success: function (x) {
                                        $('#Reports').html(x);
                                           $(document).ready(function () {
                                $('#userList').DataTable({
                                    responsive: true,
                                    lengthChange: false,
                                    "sScrollX": "100%",
                                    "scrollCollapse": true,
                                    dom: 'lBfrtip',
                                    buttons: [
                                        {
                                            extend: 'print',

                                            customize: function (win) {
                                                $(win.document.body)
                                                    .css({ 'font-size': '10pt' })
                                                    .prepend(
                                                        '<img style= src="~/Images/activefinancelogo.png"  style="position:absolute; height:500px; z-index:5000; width:500px; top:0; left:0;" />'
                                                    );

                                                $(win.document.body).find('table')
                                                    .addClass('compact')
                                                    .css('font-size', 'inherit');
                                            }
                                        }, 'excelHtml5', {
                                            extend: 'pdf',
                                            download: 'open',
                                        }
                                    ]

                                });
                            });
                                    }
                                });
                            });


                            function MyFunction() {
                                alert("asdasd");
                            }

                         

                        </script>
